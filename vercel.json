{
  "headers": [
    {
      "source": "/(.*).(webp|jpg|jpeg|png|gif|ico)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600"
        },
        {
          "key": "Content-Encoding",
          "value": "br"
        }
      ]
    }
  ]
}
